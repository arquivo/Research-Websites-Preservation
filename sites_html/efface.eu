<script>
  function setEqualHeight(columns)
  {
    var tallestcolumn = 0;
    columns.each(
    function()
    {
      currentHeight = jQuery(this).height();
      console.log(jQuery(this).attr('class')+' is the column with height: '+currentHeight);
      if(currentHeight > tallestcolumn)
      {
      	if(jQuery(this).hasClass('content-column')) {
      		tallestcolumn  = currentHeight+184; //moved slider images and pager
      	} 
        else {
      		tallestcolumn  = currentHeight;
      	}
        
      }
    });
    tallestcolumn+=0; //social media icons are added with css and seems to be  problem
    columns.height(tallestcolumn); 
  }

  jQuery(document).ready(function() {
    console.log('document.ready');
    setEqualHeight(jQuery('#columns > div'));
	});
  jQuery(document).ajaxSuccess(function() {
    console.log('document.ajaxSuccess');
    setEqualHeight(jQuery('#columns > div'));
  });
  window.onload = function() {
    console.log('window.onload');
    setEqualHeight(jQuery('#columns > div'));
  }
	
