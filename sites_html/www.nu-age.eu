    <script language="javascript" type="text/javascript">

        var UsernameWaterMarkText = 'User Name... ';
        var UserTextBoxClientId = '#ctl00_txtusername';
        var PasswordTextBoxClientId = '#ctl00_txtpassword';


        function watermark(id, watermarkText, watermarkColor, activeColor) {
            $(id).val(watermarkText).css('color', watermarkColor).focus(function () {
                if ($(this).val() == watermarkText) {
                    $(this).val('');
                    $(this).css('color', activeColor);
                }
            }).blur(function () {
                if (!$(this).val()) {
                    $(this).val(watermarkText).css('color', watermarkColor);
                }
            });
        }

        jQuery(document).ready(function () {
            var emailSent = false;
            $("#forgotpasswordcontainer").dialog({ autoOpen: false, resizable: false, width: 360, modal: true, buttons: {
                Ok: function () {
                    if (emailSent) {
                        $(this).dialog("close");
                    }
                    var emailId = $('#ctl00_txtEmailAddress').val();
                    var jsonText = JSON.stringify({ email: { Value: emailId} });
                    if (jQuery.trim(emailId).length > 0) {
                        $.ajax({
                            type: "POST",
                            url: '/forgottenpassword.aspx/ForgotenPassword',
                            contentType: "application/json; charset=utf-8",
                            data: jsonText,
                            dataType: "json",
                            success: function (data) {
                                emailSent = true;
                                $('#forgottenPasswordFailureText').html(data.d);
                                $(".entryform").hide();
                            },
                            error: function (data) {
                                $('#forgottenPasswordFailureText').html(data.d);
                            }
                        });
                    } else {
                        $('#forgottenPasswordFailureText').html("Email should not be empty");
                    }
                }
            }
            });

            if ($.cookie("css")) {
                swithFontSizeSpecificCss($.cookie("css"));

            }

            watermark(UserTextBoxClientId, UsernameWaterMarkText, '#ccc', '#000');
            watermark(PasswordTextBoxClientId, 'Password ', '#ccc', '#000');

            $(".header_subitem a").click(function () {
                var currentItem = $(this);
                var cssStylePath = currentItem.attr('rel');
                swithFontSizeSpecificCss(cssStylePath);
                $.cookie("css", cssStylePath);
                return false;
            });


            function swithFontSizeSpecificCss(cssPath) {
                var sizeCssLink = $('head link[name="sizecss"]');
                if (cssPath == "") {
                    if (sizeCssLink.length > 0) {
                        sizeCssLink.remove();
                    }
                } else {
                    if (sizeCssLink.length == 0) {
                        var cssLink = "<link name=\"sizecss\" rel=\"stylesheet\" type=\"text/css\" media=\"screen, print\" href=\"" + cssPath + "\" />";
                        $('head').append(cssLink);
                    } else {
                        sizeCssLink.attr("href", cssPath);
                    }
                }
            }

            $(".forgotpasswordlink").click(function () {
                $(".entryform").show();
                $('#forgottenPasswordFailureText').html("");
                $('#ctl00_txtEmailAddress').val("");
                $("#forgotpasswordcontainer").dialog('open');
            });          
        });


        // This will be invoked before form submit. Method registered using the Script manager
        function beforeFormSubmit() {
