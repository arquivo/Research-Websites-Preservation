					alert(cHeight);
				}
				*/
				if($(this).height() > maxHeight){
					maxHeight = $(this).height()+10;
				}
			});
			if ($(columnclass).hasClass('left')){
				addclass = 'left';
			}else{
				addclass = 'right';
			}
			newclass = columnclass.substr(1) + addclass + columncount;
			$(columnclass).height(maxHeight);
			$(columnclass).addClass(newclass);
		}
		
		function clearHeight(columnclass) {
			$(columnclass).each(function() {      
