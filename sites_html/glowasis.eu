		function setHeight(columnclass) {
			maxHeight = 0;
			columncount = 0;
			$(columnclass).each(function() {      
				if($(this).children().size() > 0) {//.html().length > 0) {
					columncount ++; 
				}else{
					$(this).remove();
				}
				/*
				cHeight = $(this).height();
