<script type="text/javascript">
//Para Noticias
	$(document).ready(function(){
		$('#listaMeses li a').on('click', function(){
			mes = $(this).parent().attr('value');
			textoMes= $(this).html();
			anio = $("#cajaSelectAnios .visto").html();
			textoAnio= anio;
			var $contenidoAjax = $('div#cont_news').html('<p style="text-align: center;"><img alt="Cargando" src="http://nanopyme-project.eu/estilos/images/ajax-loader.gif" /></p>');
			$.ajax({
				data:{fMes : mes , fAnio : anio},
				type: "POST",
				cache: false,
			    url : 'http://nanopyme-project.eu/include/cargaNoticias.php',
		        success : function (data){
		        	$contenidoAjax.html(data);
		        }
			})
			if(textoMes == "Choose month"){
					var mesVisto = new Date();
					mesVisto = mesVisto.getMonth() + 1;
					if (mesVisto=="01"){mesVisto="January";	}
					if (mesVisto=="02"){mesVisto="February";}
					if (mesVisto=="03"){mesVisto="March";	}
