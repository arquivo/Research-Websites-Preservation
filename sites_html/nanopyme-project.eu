<script type="text/javascript">
function envioForm(){
	var options = {
	    	type:			'POST',   	
	    	clearForm:		true,
	    	resetForm:		true,
	    	beforeSubmit:  	showRequest,
	        success:       	showResponse  // post-submit callback 
	    };  
	    // bind form using 'ajaxForm' 
	    $('#fContacto').ajaxForm(options);
	    function showRequest() { 
			$('.cargaMail').show();
	        return true; 
	    }    
	    function exito(){
	    	$('.cargaMail').hide();
	    	$('.cajaCorfirma').fadeIn('slow').delay(4000).fadeOut('slow');
	     };
	     function error(){
	    	$('.cargaMail').hide();
	    	$('.cajaError').fadeIn('slow').delay(4000).fadeOut('slow');
	      };    
	    function showResponse(data)  {
	    	$('.cargaMail').hide();
	    	$('#valorRespuesta').attr('value', data);     	
	    	var valor = $('#valorRespuesta').attr('value');
	    	if(valor==1){
				exito();
	        	}
	    	if(valor==0){
				error();
	        	}	 
	    }
	
}
</script></head><!-- Se abre en header.php -->

