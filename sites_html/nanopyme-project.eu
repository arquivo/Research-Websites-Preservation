		attribute: "rel",
	});


	//Acordeon	
	$('.listaArchivos').first().show(function(){
		$('#archivos .topAcor').first().addClass('selected');
	});
	

	$('.topAcor').on('click',function(){
		$('.listaArchivos').slideUp('slow');
		var pulsador = $(this);
		if ($(this).next('.listaArchivos').is (':visible')){			
