					if (mesVisto=="07"){mesVisto="July";	}
					if (mesVisto=="08"){mesVisto="August";}
					if (mesVisto=="09"){mesVisto="September";}
					if (mesVisto=="10"){mesVisto="October"; }
					if (mesVisto=="11"){mesVisto="November";}
					if (mesVisto=="12"){mesVisto="December";}
				
				$('.Mnoticia').html(mesVisto);
				}else{
					$('.Mnoticia').html(textoMes);
				}
			if(textoAnio == "Choose year"){
				var anioVisto = new Date();
				anioVisto = anioVisto.getFullYear();				
				$("#cajaSelectAnios .visto").html(anioVisto);
