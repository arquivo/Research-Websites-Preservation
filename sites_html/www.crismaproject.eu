background: #e5cbc6;
padding: 6px 5px;
border: 1px solid #fff;
border-bottom: 0;
}

.mdiv ul li a:hover{
background-color: #b4b4b4;
font-family: Verdana, sans-serif;
font-size: 8pt; 
font-weight: bold;
color: #FFFFFF;
}

.mdiv .subfolderstyle{
