.mdiv ul li{
position: relative;
}
	
/*Sub level menu items */
.mdiv ul li ul{
position: absolute;
width: 200px; /*sub menu width*/
top: 0;
visibility: hidden;
}

/* Sub level menu links style */
.mdiv ul li a{
display: block;
overflow: auto; /*force hasLayout in IE7 */
text-decoration: none;
font-family: Verdana, sans-serif;
font-size: 8pt; 
font-weight: bold;
