    },

    yAxis: [{ // Primary yAxis - PATIENTS
        title: {
            rotation: 270,
            text: 'Number of patients'
        },
        labels: {
            format: '{value}',
            style: {
                "fontWeight": "bold",
                fontSize: "15px"
            }
        }
    }, { // Secondary yAxis - SITES
        title: {
            rotation: 270,
            text: 'Number of sites'
        },
        min: 0,
        max: 80,
        tickInterval: 20,
        gridLineWidth: 1,
        gridLineDashStyle: "dot",
        labels: {
            format: '{value}',
            style: {
