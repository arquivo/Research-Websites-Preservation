    ]

}

$.getJSON("patients/statistics", function(json){
    chartOptions.xAxis.categories = json[0]['data'];
    chartOptions.series[0].data = getSumTotal(json[1]['data']);
    //chartOptions.series[0].data = json[1]['data'];
    chartOptions.series[1].data = getSumTotal(json[2]['data']);
    //chartOptions.series[1].data = json[2]['data'];
    chartOptions.series[2].data = getSumTotal(json[3]['data']);
    chart = new Highcharts.Chart(chartOptions);

    $('#patientscount').html(chartOptions.series[0].data[chartOptions.series[0].data.length -1] + chartOptions.series[1].data[chartOptions.series[1].data.length -1]);
    $('#sitescount').html(chartOptions.series[2].data[chartOptions.series[2].data.length -1]);
});



