				});
			}
		else {
			currentLayer++;
			hiddenlayer = '#mainImage1';
			$('#mainImage2').attr('src',displayimagesrc);
			
			$('#main2').show();
			$('#main1').fadeOut(600,function(){
				$(hiddenlayer).attr('src',nextimagesrc);
				});
			}	
		if(currentimage==lastImage) currentimage = 0; 
		else currentimage++;
	
		}
