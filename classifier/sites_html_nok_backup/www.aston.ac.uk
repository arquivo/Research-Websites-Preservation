 <div class="scroller-controls"><ul id="liSelector" class="clearfix"></ul></div>
	<script type="text/javascript">
	var intervalID = null;
	var i = 0;

	function next(){ show(++i); }
	function prev(){ show(--i); }

	function show(x)
	{
		if(x<0){x=$(scrollID+' ul li').length-1;}

		i=x;
		leftpx = $(scrollID+' ul li.active').outerWidth()+'px';

		$(scrollID+' ul li.active').addClass('inactive').removeClass('active').animate({left: leftpx, opacity:0}, function(){$(this).css({left:'-'+leftpx});});
		$('#liSelector li').removeClass('liCurrent');

		if(i> ($(scrollID+' ul li').length-1) ) {i=0;}

		$(scrollID+' ul li').eq(i).animate({left: 0, opacity:1}).addClass('active').removeClass('inactive');
		$('#liSelector li').eq(i+1).addClass('liCurrent');
	}

	function clickDelay()
	{
		clearInterval(intervalID);
		intervalID = setInterval("next()", 4500);
	}

	$(document).ready(function()
	{
		$(scrollID+' ul').addClass('scroller');
		$(scrollID+' ul li').addClass('inactive').css({left:'-1038px'});
		$(scrollID+' ul li').eq(0).addClass('active').removeClass('inactive').css({left:0});
		$(scrollID+' ul').mouseover(function(){clearInterval(intervalID);}).mouseleave(function(){intervalID = setInterval("next()", 4500);});

		if($('#liSelector').length>0)
		{
			$('#liSelector').addClass( scrollID.substring(1) );
			$('#liSelector').append('<li class="prev">prev</li>');
			$('#liSelector li.prev').click( function(){clickDelay(); prev();} );

			$(scrollID+' ul li').each( function(i,j)
			{
				$('#liSelector').append('<li>'+i+'</li>');
				$('#liSelector li').eq(i+1).click( function(){clickDelay(); show(i);} )
			});

			$('#liSelector').append('<li class="next">next</li>');
			$('#liSelector li.next').click( function(){clickDelay(); next();} );
		}

		//next()
		intervalID = setInterval("next()", 4500);
	});
	</script>
