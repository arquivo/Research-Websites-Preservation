<script type="text/javascript">
    $(document).ready(function () {
        

        function RegisterShowHide(inputField, field, showValue) {
            $(field).hide();

            $('input:checked', inputField).each(function () {
                show(this, field, showValue);
            });

            $('input', inputField).change(function () {
                show(this, field, showValue);
            });
        }

        function show(el, field, showValue) {
            var val = $(el).val();
            if (val == showValue) {
                $(field).show();
            }
            else {
                $(field).hide();
            }
        }

