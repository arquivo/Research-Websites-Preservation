<script type="text/javascript">
	$(document).ready(function () {
		$('#list48 .item').each(function () { resetBanner(this) });
		animateBanner();
	});

	var activeBanner = null;
	function animateBanner() {
		var nextBanner = null;
		if (activeBanner == null) {
			nextBanner = $('#list48 .item:first').eq(0);
		}
		else {
			removeBanner(activeBanner, 1000);
			nextBanner = $(activeBanner).next();
			if (nextBanner.length == 0) {
				nextBanner = $('#list48 .item:first').eq(0);
			}
		}

		startBannerAnimation(nextBanner, 1000);
		activeBanner = nextBanner;
			
		setTimeout(function () {
			animateBanner();
		}, 6000);
	}

	function resetBanner(banner) {
		$(banner).css({ left: '-1000px' });
		$('.info', banner).css({ top: '-350px' });
	}

	function startBannerAnimation(banner, speed) {
		$(banner).animate({ left: '0px' }, speed, 'swing');
		$('.info', banner).delay(1100).animate({ top: '0px' }, 400, 'swing').delay(4200).animate({ top: '370px' }, 500, 'swing');
	}

	function removeBanner(banner, speed) {
		$(banner).animate({ left: '1000px' }, speed, 'swing', function () { resetBanner(this); });
	}
</script>
