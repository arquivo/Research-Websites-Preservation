    function createCookie(name,value,days) {
        if (days) {
            var date = new Date();
            date.setTime(date.getTime()+(days*24*60*60*1000));
            var expires = "; expires="+date.toGMTString();
        }
        else var expires = "";

        
        var siteaccess = "internet6_national_multisward";
        

            document.cookie = name+"="+value+expires+"; path=/" ;
    }
            $(function() {
                var lastScrollTop = 0;

                $(window).scroll(function(event){
                    var st = $(this).scrollTop();
                    var pos_bar = $('#cookiebar').offset().top;
                    var pos_last = $('#footer_pages').offset().top;
                    var height_bar = $('#cookiebar').height();

                    if (st > lastScrollTop){
                        // downscroll code
                        if (pos_last - (pos_bar + height_bar) <= 0) {
                            $('#cookiebar').css('position','relative');
                        } else {
                            $('#cookiebar').css('position','fixed');
                        }
                    } else {
                        // upscroll code
                        if (pos_last - (st + $(this).height() + height_bar / 2) > 0) {
                            $('#cookiebar').css('position', 'fixed');
                        }
                    }
                    lastScrollTop = st;
                });

            });
    </script>

