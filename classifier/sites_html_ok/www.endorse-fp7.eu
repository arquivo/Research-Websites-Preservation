        <div id="node-1" class="node node-page odd full-node clearfix" about="/home" typeof="sioc:Item foaf:Document">
  
      <span property="dc:title" content="ENDORSE FP7 project" class="rdf-meta element-hidden"></span>
  
      <div id="node-top" class="node-top region nested">
