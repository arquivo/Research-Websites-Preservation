    function milesecure2050CallBackFooter(twitters) {
        statusHTML = [];
        for (i = 0; i < twitters.length; i++) {
            username = twitters[i].user.screen_name;
            tweet = twitters[i].text;
            tweet = tweet.replace(/((https?|s?ftp|ssh)\:\/\/[^"\s\<\>]*[^.,;'">\:\s\<\>\)\]\!])/g,function (url) {
                return '<a href="' + url + '">' + url + '</a>';
            }).replace(/\B@([_a-z0-9]+)/ig, function (reply) {
                    return  reply.charAt(0) + '<a href="http://twitter.com/' + reply.substring(1) + '">' + reply.substring(1) + '</a>';
                });

            statusHTML.push('<p><a href="http://twitter.com/' + username + '/statuses/' + twitters[i].id_str + '"><span class="tweet_time">' + relative_time(twitters[i].created_at) + '</span></a> <span class="tweet_text">' + tweet + '</span></p>');
        }
        $('#tweets_milesecure2050_footer').html(statusHTML.join(''));
    }
</script>
