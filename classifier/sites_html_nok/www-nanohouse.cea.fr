<script language=javascript>
		var news, imgs,links,dates,targs,maxNews;
		
		news=new Array();
		imgs=new Array();
		links=new Array();
		dates=new Array();
		targs=new Array();
			news[0]="Kick-off Meeting, January 19th-20th 2010, Grenoble, France";
			imgs[0]="";
			links[0]="";
			dates[0]="";
			targs[0]="MAIN";
			maxNews=1;


        var newsHeight=92;
        var newsWidth=document.getElementById("newsticker").style.width;
       	var newsPos=0;
        var newsCurrentPos=0;
        var newsMaxPos=92;
	var internalHTML="";
	
	if (maxNews==0){
  		maxNews=1;
		news[0]="<br><br><center>pas<br> d'actualités.";
		imgs[0]="";
		links[0]="";
		dates[0]="";
		targs[0]="";
	}

	for (currNews=0;currNews<maxNews;currNews++){
		internalHTML+=getHtmlCodeNews(currNews);
	}
	internalHTML+=getHtmlCodeNews(0);

	document.getElementById('newsticker').innerHTML="<div id='newsContener' style='position:absolute;width:"+newsWidth+";'></div>";
	document.getElementById('newsContener').innerHTML=internalHTML;

	function getHtmlCodeNews(curNews){
		var dc;
		dc="<div style='height:92;width:100%;overflow=hidden;'>";
		dc+="<table style='border-bottom: 1px solid #777777;border-top: 1px solid #777777' width=100% height=90 border=0 cellpading=0 cellspacing=0>";
		dc+="<tr height=90><td width=100% >";
		if(imgs[curNews]!=""){
			dc+="<table width=100% height=90 border=0 cellpading=0 cellspacing=0>";
			dc+="<tr height=85><td><img onload='if(this.width>60) this.style.width=60;if(this.height>80) this.style.height=80;' src='"+imgs[curNews]+"'></td><td valign=top><table border=0 cellspacing=0 cellpadding=0>";
			if(dates[curNews]!="")
				dc+="<tr><td align=center class=paratextsmall><i><b></b></i>"+dates[curNews]+"</td></tr>";
			dc+="<tr height=55><td class='paratext'>"+news[curNews]+"</td></tr>";
			if(links[curNews]!="")
				dc+="<tr><td align=right class=linktextsmall><a href='"+links[curNews]+"' target='"+targs[curNews]+"'><img border=0 src='/home/lib/img//linksmall.gif'> more...</td></tr>";
			dc+="</table></td></tr></table>";
		}else{
			dc+="<table width=100% height=90 border=0 cellspacing=0 cellpadding=0>";
			if(dates[curNews]!="")
				dc=dc+"<tr><td align=center class=paratextsmall><i><b>"+dates[curNews]+"</b></i></td></tr>";
			dc=dc+"<tr  height=55><td class='paratext'>"+news[curNews]+"</td></tr>";
			if(links[curNews]!="")
				dc=dc+"<tr><td align=right class=linktextsmall><a href='"+links[curNews]+"' target='"+targs[curNews]+"'><img border=0 src='/home/lib/img//linksmall.gif'> more...</td></tr>";
			dc=dc+"</table>";
		}
		dc+="</td></tr></table></div>";
		return dc;
	}


	function loopNews(){
	    if (newsCurrentPos>=newsHeight){
	    	pauseImage();
		return;
	    }
    	    newsCurrentPos+=1;
    	    newsPos+=1;
    	    document.getElementById('newsContener').style.top=-newsPos-2;
          setTimeout ("loopNews()",40);
        }
 
	function pauseImage(){
   		if (newsPos>=newsMaxPos) newsPos-=newsMaxPos;
   		newsCurrentPos-=newsHeight;
   		setTimeout("loopNews()",3000);
 	}
	
	newsCurrentPos=newsHeight;
	newsPos=0;
      document.getElementById('newsContener').style.top=-newsPos-2;
	pauseImage();
			
   </script>
