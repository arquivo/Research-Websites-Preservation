                function SelectNextSlide(stopSlideShow) {
                    if (stopSlideShow) {
                        StopSlideShow();
                    }

                    var slider = $find('ctl00_cp1_Blocks_drBlocks_ctl00_ucNewsRotator_RadSlider1');

                    var slideNumber = currentSlideNumber + 1;
                    if (slideNumber > (slidesCount - 1)) {
                        slideNumber = 0;
