                        toEnable = true;

                        window.clearInterval(slideInterval);
                        slideInterval = null;

                        button.title = 'Play Slideshow';
                        button.className = 'play';
                    }

                    var slider = $find('ctl00_cp1_Blocks_drBlocks_ctl00_ucNewsRotator_RadSlider1');
                    slider.set_enabled(toEnable);
                    slider.repaint();
                }
