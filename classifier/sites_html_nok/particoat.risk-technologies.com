                function TogglePlaySlideShow(button) {
                    var toEnable = false;
                    if (button.title == 'Play Slideshow') {
                        slideInterval = window.setInterval('SelectNextSlide()', 5000);

                        button.title = 'Stop Slideshow';
                        button.className = 'stop';
